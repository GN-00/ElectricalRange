<UserControl
    x:Class="ProjectsNow.Printing.JobOrderPages.NotificationForInspection"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="21cm"
    Height="29.7cm"
    FontFamily="Times New Roman"
    Loaded="UserControl_Loaded"
    mc:Ignorable="d">
    <Grid Margin="20" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="60" />
            <RowDefinition Height="140" />
            <RowDefinition Height="110" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="40" />
            <RowDefinition />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>

        <TextBlock
            Grid.Row="1"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            FontSize="30"
            FontWeight="Bold"
            Foreground="#4f81bd"
            Text="NOTIFICATION FOR INSPECTION" />

        <Grid
            Grid.Row="2"
            Margin="0,0,20,0"
            HorizontalAlignment="Right"
            VerticalAlignment="Center">
            <Grid.RowDefinitions>
                <RowDefinition Height="0" />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="auto" />
            </Grid.ColumnDefinitions>

            <Grid.Resources>
                <Style x:Key="gridLabels" TargetType="TextBlock">
                    <Setter Property="VerticalAlignment" Value="Center" />
                    <Setter Property="Margin" Value="10,0" />
                    <Setter Property="FontSize" Value="18" />
                    <Setter Property="HorizontalAlignment" Value="Right" />
                </Style>
            </Grid.Resources>


            <TextBlock
                Grid.Row="1"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="Date" />
            <Border
                Grid.Row="1"
                Grid.Column="1"
                MinWidth="220"
                MaxWidth="550"
                BorderBrush="Black"
                BorderThickness="1,1,1,0">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.Date, StringFormat='dd-MM-yyyy'}" />
            </Border>

            <TextBlock
                Grid.Row="2"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="Project" />
            <Border
                Grid.Row="2"
                Grid.Column="1"
                MinWidth="220"
                BorderBrush="Black"
                BorderThickness="1,1,1,0">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.ProjectName}" />
            </Border>

            <TextBlock
                Grid.Row="3"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="Company Name" />
            <Border
                Grid.Row="3"
                Grid.Column="1"
                MinWidth="220"
                BorderBrush="Black"
                BorderThickness="1,1,1,0">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.CustomerName}" />
            </Border>

            <TextBlock
                Grid.Row="4"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="Attention" />
            <Border
                Grid.Row="4"
                Grid.Column="1"
                MinWidth="220"
                BorderBrush="Black"
                BorderThickness="1,1,1,0">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.ContactName}" />
            </Border>

            <TextBlock
                Grid.Row="5"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="PO No." />
            <Border
                Grid.Row="5"
                Grid.Column="1"
                MinWidth="220"
                BorderBrush="Black"
                BorderThickness="1,1,1,0">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.PO}" />
            </Border>

            <TextBlock
                Grid.Row="6"
                Grid.Column="0"
                Style="{StaticResource gridLabels}"
                Text="JOb Order No." />
            <Border
                Grid.Row="6"
                Grid.Column="1"
                MinWidth="220"
                BorderBrush="Black"
                BorderThickness="1,1,1,1">
                <TextBlock
                    Margin="5,0"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Text="{Binding RequestData.JobOrderCode}" />
            </Border>

        </Grid>

        <TextBlock
            Grid.Row="3"
            Margin="40,5"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            FontSize="18"
            TextWrapping="Wrap">
            <Run Text="Gentlemen," />
            <LineBreak />
            <Run Text="     Regarding the information mentioned above " />
            <Run Foreground="Red" Text="E" /><Run Foreground="{StaticResource Blue}" Text="R-PCAPS" />
            <Run Text="would like to inform you that your following panels is be ready for inspection &amp; shipping So, please accept our invitation for inspection with thanks." />
        </TextBlock>

