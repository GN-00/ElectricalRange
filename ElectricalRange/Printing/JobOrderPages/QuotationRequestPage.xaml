        <Image Source="/GN.Base;component/Images/Icons/ER.png"
               RenderOptions.BitmapScalingMode="HighQuality"
               VerticalAlignment="Center"
               HorizontalAlignment="Center">
        </Image>

        <TextBlock Text="Material List"
                   Grid.Row="1"
                   FontSize="24"
                   FontWeight="Bold"
                   VerticalAlignment="Center"
                   HorizontalAlignment="Center">
        </TextBlock>


        <Grid Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3.75cm"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="3cm"/>
                <ColumnDefinition Width="3.5cm"/>
            </Grid.ColumnDefinitions>

            <StackPanel Orientation="Horizontal"
                        VerticalAlignment="Center"
                        Grid.ColumnSpan="2">
                <StackPanel.Resources>
                    <Style TargetType="{x:Type TextBlock}">
                        <Setter Property="FontSize" Value="18"/>
                    </Style>
                </StackPanel.Resources>
                <TextBlock Text="Date: "/>
                <TextBlock Text="{Binding RequestData.Date, StringFormat=dd/MM/yyyy}"/>
            </StackPanel>


            <StackPanel Orientation="Horizontal"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Right"
                        Grid.Column="2"
                        Grid.ColumnSpan="2">
                <StackPanel.Resources>
                    <Style TargetType="{x:Type TextBlock}">
                        <Setter Property="FontSize" Value="18"/>
                    </Style>
                </StackPanel.Resources>
                <TextBlock Text="Sheet: "/>
                <TextBlock Text="{Binding Page}"/>
                <TextBlock Text=" / "/>
                <TextBlock Text="{Binding Pages}"/>
            </StackPanel>

            <Border BorderBrush="Black" 
                    BorderThickness="2"
                    Grid.Row="1"
                    Grid.Column="0">
                <TextBlock Text="Customer Name"
                           Margin="5,0"
                           VerticalAlignment="Center"
                           FontSize="18">
                </TextBlock>
            </Border>

