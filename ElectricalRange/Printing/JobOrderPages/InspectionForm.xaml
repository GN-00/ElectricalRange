                       Margin="5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="Customer" />
            <TextBlock Grid.Row="2"
                       Margin="5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="Project" />
            <TextBlock Grid.Row="3"
                       Margin="5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="Job Order No." />
            <TextBlock Grid.Row="4"
                       Margin="5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="Request No." />

            <TextBlock Grid.Row="0"
                       Grid.Column="1"
                       Margin="2.5,0"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text=":" />
            <TextBlock Grid.Row="1"
                       Grid.Column="1"
                       Margin="2.5,0"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text=":" />
            <TextBlock Grid.Row="2"
                       Grid.Column="1"
                       Margin="2.5,0"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text=":" />
            <TextBlock Grid.Row="3"
                       Grid.Column="1"
                       Margin="2.5,0"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text=":" />
            <TextBlock Grid.Row="4"
                       Grid.Column="1"
                       Margin="2.5,0"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text=":" />

            <TextBlock Grid.Row="0"
                       Grid.Column="2"
                       Margin="2.5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="{Binding RequestData.Date, StringFormat=dd/MM/yyyy}" />
            <TextBlock Grid.Row="1"
                       Grid.Column="2"
                       Margin="2.5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
