                       Grid.Column="2"
                       Margin="2.5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="{Binding RequestData.JobOrderCode}" />
            <TextBlock Grid.Row="4"
                       Grid.Column="2"
                       Margin="2.5,0"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontSize="14"
                       Text="{Binding RequestData.RequestCode}" />
