<UserControl x:Class="ProjectsNow.Printing.Finance.ProformaInvoiceForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="21cm"
             Height="29.7cm"
             FontFamily="Calibri (Body)"
             Loaded="UserControl_Loaded"
             mc:Ignorable="d">

    <Grid Margin="20"
          Background="White">

        <Image x:Name="BackgroundImage"
               Margin="-20"
               RenderOptions.BitmapScalingMode="HighQuality" />

        <StackPanel Orientation="Vertical">
            <Grid Height="21"
                  Margin="15,80,15,0"
                  DataContext="{Binding InvoiceData}">
                <StackPanel HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                    <TextBlock FontSize="14"
                               Foreground="#4f81bd"
                               Text="" />
                    <TextBlock FontSize="14"
                               Foreground="Black"
                               Text="" />
                </StackPanel>
                <StackPanel HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                    <TextBlock FontSize="14"
                               Foreground="Black"
                               Text="" />
                    <TextBlock FontSize="14"
                               Foreground="#4f81bd"
                               Text="" />
                    <TextBlock FontSize="14"
                               Foreground="#4f81bd"
                               Text="" />
                </StackPanel>
            </Grid>
            <Grid Margin="0,10,0,0"
                  DataContext="{Binding InvoiceData}">
                <TextBlock x:Name="FormName"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Center"
                           FontSize="26"
                           Foreground="#4f81bd"
                           Text="PROFORMA INVOICE" />
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           FontSize="26"
                           Foreground="Black"
                           Text="{Binding Code}" />
                <TextBlock x:Name="FormArabicName"
                           HorizontalAlignment="Right"
                           VerticalAlignment="Center"
                           FontSize="26"
                           Foreground="#4f81bd"
                           Text="فاتورة أولية" />
            </Grid>

            <StackPanel x:Name="DetailsArea"
                        DataContext="{Binding InvoiceData}"
                        Orientation="Vertical"
                        Visibility="Collapsed">
                <StackPanel.Resources>
                    <Style TargetType="Grid">
                        <Setter Property="Height" Value="20" />
                    </Style>
                </StackPanel.Resources>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="Date" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding Date, StringFormat=dd-MM-yyyy}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="التاريخ" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="J.O#" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding JobOrderCode}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="رقم أمر العمل" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="P.O #" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding PurchaseOrder}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="رقم أمر الشراء" />
                </Grid>

                <Grid Margin="0,10,0,0">
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="#4f81bd"
                               Text="BILL TO " />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="#4f81bd"
                               Text="فاتورة الى" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="M/s" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding CustomerNameInfo}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="السادة" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="Attention" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding Contact}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="عناية" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="Project " />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding Project}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="مشروع" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="Address" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding Address}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="العنون" />
                </Grid>

                <Grid>
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="VAT #" />
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="{Binding CustomerVATNumber}" />
                    <TextBlock HorizontalAlignment="Right"
                               VerticalAlignment="Center"
                               FontSize="14"
                               Foreground="Black"
                               Text="الضريبي الرقم" />
                </Grid>

            </StackPanel>

            <Grid Height="20"
                  Margin="0,10,0,0">
                <TextBlock HorizontalAlignment="Left"
                           VerticalAlignment="Center"
                           FontSize="14"
                           FontWeight="Bold"
                           Foreground="#4f81bd"
                           Text="INVOICE DETAILS"
                           TextDecorations="UnderLine" />
            </Grid>

            <Grid x:Name="TableArea"
                  Margin="0,5,0,0"
                  HorizontalAlignment="Center">
                <Grid.Resources>
                    <Style TargetType="Border">
                        <Setter Property="BorderBrush" Value="Black" />
                    </Style>
                </Grid.Resources>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80" />
                    <ColumnDefinition Width="300" />
                    <ColumnDefinition Width="50" />
                    <ColumnDefinition Width="80" />
                    <ColumnDefinition Width="80" />
                    <ColumnDefinition Width="80" />
                    <ColumnDefinition Width="80" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition Height="0" />
                    <RowDefinition Height="0" />
                </Grid.RowDefinitions>

                <!--#region Heaer-->

                <Border Grid.Column="0"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="1,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="S.No." />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="رقم تسلسلي" />
                    </Grid>
                </Border>
                <Border Grid.Column="1"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="Description" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="البيان" />
                    </Grid>
                </Border>
                <Border Grid.Column="2"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="Qty" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="الكمية" />
                    </Grid>
                </Border>
                <Border Grid.Column="3"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="U/Price" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="سعر الوحدة" />
                    </Grid>
                </Border>
                <Border Grid.Column="4"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="Amount" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="المبلغ" />
                    </Grid>
                </Border>
                <Border Grid.Column="5"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White">
                            <Run Text="VAT" />
                            <Run Text="{Binding VATPercentage, StringFormat=N0, Mode=OneWay}" /><Run Text="%" />
                        </TextBlock>
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="الضريبة" />
                    </Grid>
                </Border>
                <Border Grid.Column="6"
                        Padding="0,4"
                        Background="#4f81bd"
                        BorderThickness="0,1,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="T/Price" />
                        <TextBlock Grid.Row="1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Foreground="White"
                                   Text="القيمة" />
                    </Grid>
                </Border>

                <!--#endregion-->

                <!--#region Rows-->

                <!--#region Row 1-->
                <Border Grid.Row="1"
                        Grid.Column="0"
                        Background="#4f81bd"
                        BorderThickness="1,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="White"
                               Text="{Binding SN}" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="1"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0"
                                   Margin="5,2,5,0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding Description}"
                                   TextWrapping="Wrap" />
                        <TextBlock Grid.Row="1"
                                   Margin="5,2"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding ArabicDescription}" />
                    </Grid>
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="2"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding Qty}" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="3"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding UnitNetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="4"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding NetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="5"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding VATValue, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="1"
                        Grid.Column="6"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[0]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding GrossPrice, StringFormat=N2}" />
                </Border>

                <!--#endregion-->

                <!--#region Row 2-->
                <Border Grid.Row="2"
                        Grid.Column="0"
                        Background="#4f81bd"
                        BorderThickness="1,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="White"
                               Text="{Binding SN}" />
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="1"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0"
                                   Margin="5,2,5,0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding Description}"
                                   TextWrapping="Wrap" />
                        <TextBlock Grid.Row="1"
                                   Margin="5,2"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding ArabicDescription}" />
                    </Grid>
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="2"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding Qty}" />
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="3"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding UnitNetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="4"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding NetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="5"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding VATValue, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="2"
                        Grid.Column="6"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[1]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding GrossPrice, StringFormat=N2}" />
                </Border>
                <!--#endregion-->

                <!--#region Row 3-->
                <Border Grid.Row="3"
                        Grid.Column="0"
                        Background="#4f81bd"
                        BorderThickness="1,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="White"
                               Text="{Binding SN}" />
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="1"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0"
                                   Margin="5,2,5,0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding Description}"
                                   TextWrapping="Wrap" />
                        <TextBlock Grid.Row="1"
                                   Margin="5,2"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding ArabicDescription}" />
                    </Grid>
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="2"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding Qty}" />
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="3"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding UnitNetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="4"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding NetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="5"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding VATValue, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="3"
                        Grid.Column="6"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[2]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding GrossPrice, StringFormat=N2}" />
                </Border>
                <!--#endregion-->

                <!--#region Row 4-->
                <Border Grid.Row="4"
                        Grid.Column="0"
                        Background="#4f81bd"
                        BorderThickness="1,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Foreground="White"
                               Text="{Binding SN}" />
                </Border>
                <Border Grid.Row="4"
                        Grid.Column="1"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0"
                                   Margin="5,2,5,0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding Description}"
                                   TextWrapping="Wrap" />
                        <TextBlock Grid.Row="1"
                                   Margin="5,2"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontSize="14"
                                   FontWeight="Bold"
                                   Text="{Binding ArabicDescription}" />
                    </Grid>
                </Border>
                <Border Grid.Row="4"
                        Grid.Column="2"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding Qty}" />
                </Border>
                <Border Grid.Row="4"
                        Grid.Column="3"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding UnitNetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="4"
                        Grid.Column="4"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
                    <TextBlock Margin="5,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="14"
                               FontWeight="Bold"
                               Text="{Binding NetPrice, StringFormat=N2}" />
                </Border>
                <Border Grid.Row="4"
                        Grid.Column="5"
                        BorderThickness="0,0,1,1"
                        DataContext="{Binding PanelsData[3]}">
