                           FontWeight="Bold"
                           Text="{Binding PageNumber}" />
                <TextBlock FontSize="14"
                           Text=" / " />
                <TextBlock FontSize="14"
                           FontWeight="Bold"
                           Text="{Binding TotalPages}" />
            </StackPanel>
        </Grid>

    </Grid>
</UserControl>
