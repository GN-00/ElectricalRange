<UserControl x:Class="ProjectsNow.Printing.Finance.PanelsInvoiceForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Loaded="UserControl_Loaded"
             FontFamily="Calibri (Body)"
             Height="29.7cm" 
             Width="21cm">
    
    <Grid Margin="20"
          Background="White">
        
        <Image Margin="-20" 
               x:Name="BackgroundImage"
               RenderOptions.BitmapScalingMode="HighQuality">
        </Image>

        <StackPanel Orientation="Vertical">
            <Grid Height="21" 
                  Margin="15,80,15,0"
                  DataContext="{Binding InvoiceData}">
                <StackPanel Orientation="Horizontal"  HorizontalAlignment="Left" VerticalAlignment="Center">
                    <TextBlock Text="" Foreground="#4f81bd" FontSize="14"/>
                    <TextBlock Text="" Foreground="Black" FontSize="14"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal"  HorizontalAlignment="Right" VerticalAlignment="Center">
                    <TextBlock Text="" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="" Foreground="#4f81bd" FontSize="14"/>
                    <TextBlock Text="" Foreground="#4f81bd" FontSize="14"/>
                </StackPanel>
            </Grid>
            <Grid DataContext="{Binding InvoiceData}" Margin="0,10,0,0">
                <TextBlock x:Name="FormName" Text="VAT INVOICE" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#4f81bd" FontSize="26"/>
                <TextBlock Text="{Binding InvoiceNumber}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="26"/>
                <TextBlock x:Name="FormArabicName" Text="فاتورة ضريبية" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="#4f81bd" FontSize="26"/>
            </Grid>
            
            <StackPanel x:Name="DetailsArea" 
                        Visibility="Collapsed"
                        Orientation="Vertical"
                        DataContext="{Binding InvoiceData}">
                <StackPanel.Resources>
                    <Style TargetType="Grid">
                        <Setter Property="Height" Value="20"/>
                    </Style>
                </StackPanel.Resources>

                <Grid>
                    <TextBlock Text="Date" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding Date, StringFormat=yyyy-MM-dd}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="التاريخ" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>

                <Grid>
                    <TextBlock Text="J.O#" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding JobOrderCode}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="رقم أمر العمل" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>

                <Grid>
                    <TextBlock Text="P.O #" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding PurchaseOrderNumber}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="رقم أمر الشراء" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>
                
                <Grid Margin="0,10,0,0">
                    <TextBlock Text="BILL TO " HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#4f81bd" FontWeight="Bold" FontSize="14"/>
                    <TextBlock Text="فاتورة الى" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="#4f81bd" FontWeight="Bold" FontSize="14"/>
                </Grid>
                
                <Grid>
                    <TextBlock Text="M/s" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding CustomerNameInfo}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="السادة" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>
                
                <Grid>
                    <TextBlock Text="Attention" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding Attention}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding Contact}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="عناية" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>
                
                <Grid>
                    <TextBlock Text="Project " HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding ProjectName}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="مشروع" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>
                
                <Grid>
                    <TextBlock Text="Address" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding Address}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="العنون" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>
                
                <Grid>
                    <TextBlock Text="VAT #" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="{Binding CustomerVAT}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                    <TextBlock Text="الضريبي الرقم" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="Black" FontSize="14"/>
                </Grid>

            </StackPanel>
            
            <Grid Height="20" Margin="0,10,0,0">
                <TextBlock Text="INVOICE DETAILS" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#4f81bd" FontWeight="Bold" FontSize="14" TextDecorations="UnderLine"/>
            </Grid>

            <Grid x:Name="TableArea"  
                  Margin="0,5,0,0" 
                  HorizontalAlignment="Center">
                <Grid.Resources>
                    <Style TargetType="Border">
                        <Setter Property="BorderBrush" Value="Black"/>
                    </Style>
                </Grid.Resources>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="300"/>
                    <ColumnDefinition Width="50"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="80"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition Height="0"/>
                </Grid.RowDefinitions>
                
                <!--#region Heaer-->

                <Border Grid.Column="0" 
                        Padding="0,4"
                        BorderThickness="1,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="S.No." 
                                    FontSize="14"
                                    FontWeight="Bold" 
                                    Foreground="White"
                                    VerticalAlignment="Center" 
                                    HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="رقم تسلسلي" 
                                    Grid.Row="1"
                                    FontSize="14"
                                    FontWeight="Bold" 
                                    Foreground="White"
                                    VerticalAlignment="Center" 
                                    HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="1" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="Description" 
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="البيان" 
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="2" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="Qty" 
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="الكمية" 
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="3" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="U/Price" 
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="سعر الوحدة"  
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="4" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="Amount" 
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="المبلغ" 
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="5" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                            <Run Text="VAT"/>
                            <Run Text="{Binding VATPercentage, StringFormat=N0, Mode=OneWay}"/><Run Text="%"/>
                        </TextBlock>
                        <TextBlock Text="الضريبة" 
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border Grid.Column="6" 
                        Padding="0,4"
                        BorderThickness="0,1,1,1" 
                        Background="#4f81bd">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="T/Price" 
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                        <TextBlock Text="القيمة" 
                                   Grid.Row="1"
                                   FontSize="14"
                                   FontWeight="Bold" 
                                   Foreground="White"
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Center">
                        </TextBlock>
                    </Grid>
                </Border>
                
                <!--#endregion-->

                <!--#region Rows-->

                <!--#region Row 1-->
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" 
                        Grid.Column="0" 
                        BorderThickness="1,0,1,1" 
                        Background="#4f81bd">
                    <TextBlock Text="{Binding SN}" 
                               FontSize="14" 
                               FontWeight="Bold" 
                               Foreground="White"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}"
                        Grid.Row="1" Grid.Column="1" 
                        BorderThickness="0,0,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Description}" 
                                   Grid.Row="0"
                                   Margin="5,2,5,0"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   TextWrapping="Wrap" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Left" >
                        </TextBlock>
                        <TextBlock Text="{Binding ArabicDescription}"  
                                   Grid.Row="1"
                                   Margin="5,2"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Right">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" Grid.Column="2" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding Qty}" 
                               FontSize="14" 
                               Margin="5,0"
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" Grid.Column="3" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding UnitNetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" Grid.Column="4" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding NetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" Grid.Column="5" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding VATValue, StringFormat=N2}"                      
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[0]}" 
                        Grid.Row="1" Grid.Column="6" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding GrossPrice, StringFormat=N2}"                                
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                
                <!--#endregion-->

                <!--#region Row 2-->
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" 
                        Grid.Column="0" 
                        BorderThickness="1,0,1,1" 
                        Background="#4f81bd">
                    <TextBlock Text="{Binding SN}" 
                               FontSize="14" 
                               FontWeight="Bold" 
                               Foreground="White"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}"
                        Grid.Row="2" Grid.Column="1" 
                        BorderThickness="0,0,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Description}" 
                                   Grid.Row="0"
                                   Margin="5,2,5,0"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   TextWrapping="Wrap" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Left" >
                        </TextBlock>
                        <TextBlock Text="{Binding ArabicDescription}"  
                                   Grid.Row="1"
                                   Margin="5,2"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Right">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" Grid.Column="2" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding Qty}" 
                               FontSize="14" 
                               Margin="5,0"
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" Grid.Column="3" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding UnitNetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" Grid.Column="4" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding NetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" Grid.Column="5" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding VATValue, StringFormat=N2}"                      
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[1]}" 
                        Grid.Row="2" Grid.Column="6" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding GrossPrice, StringFormat=N2}"                                
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <!--#endregion-->

                <!--#region Row 3-->
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" 
                        Grid.Column="0" 
                        BorderThickness="1,0,1,1" 
                        Background="#4f81bd">
                    <TextBlock Text="{Binding SN}" 
                               FontSize="14" 
                               FontWeight="Bold" 
                               Foreground="White"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}"
                        Grid.Row="3" Grid.Column="1" 
                        BorderThickness="0,0,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Description}" 
                                   Grid.Row="0"
                                   Margin="5,2,5,0"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   TextWrapping="Wrap" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Left" >
                        </TextBlock>
                        <TextBlock Text="{Binding ArabicDescription}"  
                                   Grid.Row="1"
                                   Margin="5,2"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Right">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" Grid.Column="2" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding Qty}" 
                               FontSize="14" 
                               Margin="5,0"
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" Grid.Column="3" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding UnitNetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" Grid.Column="4" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding NetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" Grid.Column="5" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding VATValue, StringFormat=N2}"                      
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[2]}" 
                        Grid.Row="3" Grid.Column="6" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding GrossPrice, StringFormat=N2}"                                
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <!--#endregion-->

                <!--#region Row 4-->
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" 
                        Grid.Column="0" 
                        BorderThickness="1,0,1,1" 
                        Background="#4f81bd">
                    <TextBlock Text="{Binding SN}" 
                               FontSize="14" 
                               FontWeight="Bold" 
                               Foreground="White"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}"
                        Grid.Row="4" Grid.Column="1" 
                        BorderThickness="0,0,1,1">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Description}" 
                                   Grid.Row="0"
                                   Margin="5,2,5,0"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   TextWrapping="Wrap" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Left" >
                        </TextBlock>
                        <TextBlock Text="{Binding ArabicDescription}"  
                                   Grid.Row="1"
                                   Margin="5,2"
                                   FontSize="14" 
                                   FontWeight="Bold" 
                                   VerticalAlignment="Center" 
                                   HorizontalAlignment="Right">
                        </TextBlock>
                    </Grid>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" Grid.Column="2" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding Qty}" 
                               FontSize="14" 
                               Margin="5,0"
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" Grid.Column="3" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding UnitNetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" Grid.Column="4" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding NetPrice, StringFormat=N2}" 
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" Grid.Column="5" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding VATValue, StringFormat=N2}"                      
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <Border DataContext="{Binding PanelsData[3]}" 
                        Grid.Row="4" Grid.Column="6" 
                        BorderThickness="0,0,1,1">
                    <TextBlock Text="{Binding GrossPrice, StringFormat=N2}"                                
                               Margin="5,0"
                               FontSize="14" 
                               FontWeight="Bold" 
                               VerticalAlignment="Center" 
                               HorizontalAlignment="Center">
                    </TextBlock>
                </Border>
                <!--#endregion-->

                <!--#region Row 5-->
                <Border DataContext="{Binding PanelsData[4]}" 
                        Grid.Row="5" 
                        Grid.Column="0" 
                        BorderThickness="1,0,1,1" 
                        Background="#4f81bd">
                    <TextBlock Text="{Binding SN}" 
                               FontSize="14" 
                               FontWeight="Bold" 
                               Foreground="White"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center">
